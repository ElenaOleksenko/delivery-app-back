{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
  },
  "extends": [
          "airbnb"
  ],
  "parserOptions": {
      "ecmaVersion": 12
  },
  
   
 
  "rules": {
    "no-tabs": 0, 
  "no-console": "off",
  "camelcase": "off",
  "no-return-await": "off",
  "prefer-regex-literals": "off",
  "consistent-return": "off",

      
  "no-use-before-define": [
      "error",
      "nofunc"
    ],
    "no-unused-vars": "off",
    "no-underscore-dangle": "off",
    "linebreak-style": 0
  }

 
}
